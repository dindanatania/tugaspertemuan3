package tugas3.no2;

import java.util.Scanner;

public class Tugas3No2 {
    public static void main(String[] args) {
        
        Scanner input = new Scanner(System.in);
        
        System.out.println("Masukkan sisi ke-1: ");
        int sisi1 = input.nextInt();
        
        System.out.println("Masukkan sisi ke-2: ");
        int sisi2 = input.nextInt();
        
        System.out.println("Masukkan sisi ke-3: ");
        int sisi3 = input.nextInt();

        if(sisi1 == sisi2 && sisi2 == sisi3){
            System.out.println("Termasuk Kubus");
        }else{
            System.out.println("Bukan termasuk Kubus");
        }
    }  
}
